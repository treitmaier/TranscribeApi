addUser:
INSERT INTO User_Entity(email, password) VALUES (?,?) RETURNING id;

addUserReturning:
INSERT INTO User_Entity(id, email, password, created_at) VALUES (?,?, ?, ?) RETURNING id;

findUserByEmail:
SELECT * FROM User_Entity WHERE email = ? LIMIT 1;

findUserByEmailAndPassword:
SELECT * FROM User_Entity WHERE email = ? AND password = ? LIMIT 1;
